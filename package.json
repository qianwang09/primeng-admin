    "start:express": "node build\\express-server.js",    
    "start:webpack": "webpack-dev-server --inline --progress --port 8080",
    "test:webpack": "karma start karma.webpack.config.js",
    "build:webpack": "rimraf dist\\ && webpack --config build/webpack.prod.config.js --bail",
    "build:webpack1": "webpack --config config/webpack.prod.js --bail",
    "build:babel": "babel src -d src --extensions \".es6\" --source-maps",
    "copy-dist-files": "node ./copy-dist-files.js",
    "i18n": "ng-xi18n"
